{
    "name": "%[author]%/%[package]%",
    "type": "library",
    "license": "MIT",
    "description": "%[description]%",
    "keywords": [
        "php",
        "library"
    ],
    "authors": [
        {
            "name": "Marwan Al-Soltany",
            "email": "MarwanAlsoltany+gh@gmail.com"
        },
        {
            "name": "%[authorName]%",
            "email": "%[authorEmail]%"
        }
    ],
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/%[author]%"
        }
    ],
    "homepage": "https://github.com/%[author]%/%[package]%#readme",
    "support": {
        "docs": "https://github.com/%[author]%/%[package]%/blob/master/README.md",
        "issues": "https://github.com/%[author]%/%[package]%/issues"
    },
    "require": {
        "php" : ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpbench/phpbench": "^1.2",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.9",
        "code-lts/doctum": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests"
        }
    },
    "bin": [
        "bin/executable"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": false,
        "process-timeout": 0
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
